#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/examples/node_modules/.pnpm/vercel@24.2.5_react-dom@18.2.0_react@18.2.0/node_modules/vercel/dist/node_modules:/workspaces/examples/node_modules/.pnpm/vercel@24.2.5_react-dom@18.2.0_react@18.2.0/node_modules/vercel/node_modules:/workspaces/examples/node_modules/.pnpm/vercel@24.2.5_react-dom@18.2.0_react@18.2.0/node_modules:/workspaces/examples/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/examples/node_modules/.pnpm/vercel@24.2.5_react-dom@18.2.0_react@18.2.0/node_modules/vercel/dist/node_modules:/workspaces/examples/node_modules/.pnpm/vercel@24.2.5_react-dom@18.2.0_react@18.2.0/node_modules/vercel/node_modules:/workspaces/examples/node_modules/.pnpm/vercel@24.2.5_react-dom@18.2.0_react@18.2.0/node_modules:/workspaces/examples/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vercel/dist/index.js" "$@"
else
  exec node  "$basedir/../vercel/dist/index.js" "$@"
fi
